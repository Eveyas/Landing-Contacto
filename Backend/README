-- DATA BASE 

CREATE DATABASE LandingPage;
USE LandingPage;

CREATE TABLE ContacUs (
id INT PRIMARY KEY AUTO_INCREMENT,
nombre VARCHAR (30),
apellidos VARCHAR (50),
correo VARCHAR (50),
telefono VARCHAR (15),
mensaje TEXT
);

CREATA TU PROPIO .env
DB_HOST=localhost
DB_USER=***
DB_PASSWORD=***
DB_NAME=LandingPage


correlo con 
node app.js 